{
  "name": "app-frantoi",
  "version": "0.1.0",
  "description": "Applicazione per la gestione del registro olio e l'integrazione con i servizi ICQRF",
  "main": "dist/backend/server.js",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "nodemon --exec ts-node src/backend/server.ts",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build:frontend": "vite build",
    "build:backend": "tsc -p tsconfig.json",
    "build": "npm run build:frontend && npm run build:backend",
    "start": "node dist/backend/server.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "db:create": "node scripts/create-database.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed:admin": "node scripts/create-admin-user.js",
    "db:reset:admin": "node scripts/reset-admin-user.js",
    "db:update:cisterne": "node scripts/update-cisterne-schema.js",
    "db:backup": "node scripts/backup-database.js",
    "db:restore": "node scripts/restore-database.js",
    "db:auto-backup:configure": "node scripts/configure-auto-backup.js"
  },
  "keywords": [
    "frantoio",
    "olio",
    "registro",
    "icqrf"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@iconify/react": "^6.0.0",
    "@prisma/client": "^5.22.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react-big-calendar": "^1.16.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.9",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "flowbite": "^3.1.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "moment": "^2.30.1",
    "node-soap": "^1.0.0",
    "nodemailer": "^6.10.1",
    "pg": "^8.14.1",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-big-calendar": "^1.18.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "tailwindcss": "^4.1.11",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.30",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "csv-parser": "^3.2.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "iconv-lite": "^0.6.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prisma": "^5.22.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^4.5.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
